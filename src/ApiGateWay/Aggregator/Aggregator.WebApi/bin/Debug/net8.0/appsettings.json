{
  "ApplicationName": "Aggregator.WebApi",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MSSQLConnection": ""
  },
  "JwtBearer": {
    "Authority": "https://identityserverapi-api:8081"
  },
  "GrpcServices": {
    "ChatService": {
      "Address": "http://chatwebapi-api:8081"
    },
    "UserService": {
      "Address": "http://userwebapi-api:8081"
    },
    "PostService": {
      "Address": "http://postwebapi-api:8081"
    }
  }
}


